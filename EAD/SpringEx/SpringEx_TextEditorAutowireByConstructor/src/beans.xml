<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id = "textEditor" class = "com.metacube.training.TextEditor" autowire = "constructor">
		<constructor-arg type = "SpellChecker" ref = "spellCheckerId2" /> 
		<constructor-arg type = "SpellChecker" ref = "spellCheckerId1" />
	</bean>
	<bean id = "spellCheckerId1" class = "com.metacube.training.SpellChecker">
		<constructor-arg type = "String" value = "Good" />
	</bean>
	<bean id = "spellCheckerId2" class = "com.metacube.training.SpellChecker">
		<constructor-arg type = "String" value = "Weak" />
	</bean>
</beans>